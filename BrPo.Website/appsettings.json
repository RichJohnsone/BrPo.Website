{
  "AllowedHosts": "*",
  "ClientValidationEnabled": "true",
  "FileUpload": {
    "AllowedFileExtensions": ".jpg, .jpeg, .tiff",
    "MaxAllowedSizeInMB": "1024",
    "FolderPath": "Uploads"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "UnobtrusiveJavaScriptEnabled": "true"
}
